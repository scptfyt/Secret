{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "api/check.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.jpg", "use": "@vercel/static" },
    { "src": "*.mp3", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/check.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}